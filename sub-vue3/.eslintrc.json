{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:vue/vue3-essential"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["vue"],
    "rules": {
        "semi": 1,
        "quotes": ["error", "single"],
        "vue/multi-word-component-names": "off"
    }
}
